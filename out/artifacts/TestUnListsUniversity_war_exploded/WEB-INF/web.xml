<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>ControlJSP</servlet-name>
        <servlet-class>controller.ControlJSP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControlJSP</servlet-name>
        <url-pattern>/control</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Control_panel</servlet-name>
        <servlet-class>controller.Control_panel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Control_panel</servlet-name>
        <url-pattern>/control_panel</url-pattern>
    </servlet-mapping>
</web-app>